{
  "globals": {
    "defineProps": "readonly",
    "defineEmits": "readonly",
    "defineExpose": "readonly",
    "withDefaults": "readonly",
    "computed": "readonly",
    "createApp": "readonly",
    "padStart": "readonly",
    "useRouter": "readonly",
    "reactive": "readonly",
    "ref": "readonly"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
      ]
    },
    {
      "files": [
        "*.vue"
      ],
      "parser": "vue-eslint-parser",
      "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
      },
      "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-recommended",
        "plugin:prettier/recommended"
      ],
      "rules": {
        "vue/multi-word-component-names": "off"
      }
    }
  ],
  "parserOptions": {
    "sourceType": "module"
  }
}